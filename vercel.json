{
    "builds": [{
        "src": "my_site/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9", "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate" }
    }],


    "routes": [
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        }
      ]
}


  
  
  